# MindBody API Configuration
# Copy this file to .env and fill in your actual values

# Common settings for both V5 and V6
MINDBODY_SITE_ID=your_site_id_here
MINDBODY_USERNAME=your_username_here
MINDBODY_PASSWORD=your_password_here

# V5 specific settings (SOAP API)
MINDBODY_SOURCE_NAME=your_username_here
MINDBODY_SOURCE_PASSWORD=your_password_here

# V6 specific settings
MINDBODY_API_KEY=your_api_key_here

# Optional: Test site ID for V6 (defaults to -99 if not set)
MINDBODY_TEST_SITE_ID=-99

# Application settings
NODE_ENV=development
PORT=3000

# Logging level
LOG_LEVEL=info
